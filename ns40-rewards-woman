    <!-- End Hotjar Tracking Code --><script>
      var _learnq = _learnq || [];document.addEventListener('cart:add', ({detail}) => {
          let items = [];
          if ('items' in detail) {
              items = [ ...detail.items]
          } else {
              items = [detail]
          }

          items.forEach(({title, product_type, vendor, price, product_id, image, url}) => {
              _learnq.push(['track', 'Added to Cart', {
                  Name: title,
                  ProductID: product_id,
                  Categories: product_type,
                  ImageURL: image,
                  URL: `https://shop.tropicfeel.com${url}`,
                  Brand: vendor,
                  Price: Shopify.currency.formatMoney(price)
              }]);
          })
      })

  </script>

