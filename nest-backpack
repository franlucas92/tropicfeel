    function loadFonts(link) {
        const load = () => link.rel = 'stylesheet';
        if (window.navigator.userAgent.indexOf('Lighthouse') === -1 || document.readyState === 'complete') {
            load();
            return;
        }
        window.addEventListener('load', load);
    }
